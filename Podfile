use_frameworks!
platform :ios, '9.0'

def basic_app_pods
    pod 'NeuraSDKFramework'
    pod 'OneSignal', '>= 2.11.2', '< 3.0'
    pod 'KRProgressHUD'
end

target 'Neura Swift Sample App' do
    basic_app_pods
end

target 'Notifications' do
  #only copy below line
  pod 'OneSignal', '>= 2.11.2', '< 3.0'
end
